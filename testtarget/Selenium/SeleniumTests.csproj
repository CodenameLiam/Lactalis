	</PropertyGroup>

	<ItemGroup>
		<Content Include="EdgeChromiumDriver\*.*" CopyToOutputDirectory="Always" />
		<None Include="../API/SiteConfig.ini" Link="SiteConfig.ini" CopyToOutputDirectory="PreserveNewest" />
